{
    "_version": "1.15.0",
    "sap.app": {
        "id": "sap.fe.demo.incidents",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "ach": "CA-UI5-FE",
        "dataSources": {
            "mainService": {
                "uri": "/incident/",
                "type": "OData",
                "settings": {
                    "odataVersion": "4.0"
                }
            }
        },
        "offline": false,
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "ui5template.fiorielements.v4.lrop",
            "version": "1.0.0"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "resources": {
            "js": [],
            "css": []
        },
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.fe.templates": {},
                "sap.ui.fl": {}
            }
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            },
            "": {
                "dataSource": "mainService",
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            }
        },
        "routing": {
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "SafetyIncidentsList",
                    "target": "SafetyIncidentsList"
                },
                {
                    "pattern": "SafetyIncidents({key}):?query:",
                    "name": "SafetyIncidentsObjectPage",
                    "target": "SafetyIncidentsObjectPage"
                },
                {
                    "pattern": "SafetyIncidents({key})/incidentFlow({key2}):?query:",
                    "name": "ProcessFlowViewPage_incidentFlow",
                    "target": "ProcessFlowViewPage_incidentFlow"
                }
            ],
            "targets": {
                "SafetyIncidentsList": {
                    "type": "Component",
                    "id": "SafetyIncidentsList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "SafetyIncidents",
                            "variantManagement": "Page",
                            "enhanceI18n": "i18n/customIncidentLR.properties",
                            "navigation": {
                                "SafetyIncidents": {
                                    "detail": {
                                        "route": "SafetyIncidentsObjectPage"
                                    }
                                }
                            },
                            "initialLoad": true,
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {}
                            }
                        }
                    }
                },
                "SafetyIncidentsObjectPage": {
                    "type": "Component",
                    "id": "SafetyIncidentsObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "SafetyIncidents",
                            "navigation": {
                                "incidentFlow": {
                                    "detail": {
                                        "route": "ProcessFlowViewPage_incidentFlow"
                                    }
                                }
                            },
                            "editableHeaderContent": false,
                            "content": {
                                "body": {
                                    "sections": {
                                        "CustomSectionGantt": {
                                            "name": "sap.fe.demo.incidents.ext.fragment.CustomSectionGantt",
                                            "position": {
                                                "anchor": "IncidentOverviewFacet",
                                                "placement": "After"
                                            },
                                            "type": "XMLFragment",
                                            "title": "Maximum Processing Time"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "ProcessFlowViewPage_incidentFlow": {
                    "viewId": "ProcessFlowView",
                    "viewName": "sap.fe.demo.incidents.ext.view.ProcessFlow",
                    "options": {
                        "settings": {
                            "entitySet": "IncidentFlow",
                            "navigation": {}
                        }
                    }
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        }
    },
    "sap.platform.abap": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.platform.hcp": {
        "_version": "1.1.0",
        "uri": ""
    },
    "sap.fiori": {
        "_version": "1.1.0",
        "registrationIds": [],
        "archeType": "transactional"
    }
}